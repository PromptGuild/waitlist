{
   "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": ["dom", "dom.iterable", "esnext", "scripthost"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "noImplicitReturns": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "jsx": "preserve",
      "newLine": "lf",
      "locale": "en"
   },
   "include": ["src"]
}
